import java.util.*;

public class Hw2{
   public static void main(String[] argv){
	
     System.out.println("Please input the temperature in ℃");
     Scanner tem = new Scanner(System.in);
     float temper = tem.nextFloat();
     System.out.println("The temperature is " + (temper * 9 / 5 + 32) + "℉" );
  }
}
